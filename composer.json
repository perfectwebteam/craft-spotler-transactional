{
	"name": "perfectwebteam/craft-spotler-transactional",
	"description": "Spotler Transactional mailer adapter for Craft CMS.",
	"type": "craft-plugin",
    "version": "1.0.0",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"spotler transactional",
		"spotler",
		"transactional"
	],
	"support": {
		"docs": "https://github.com/perfectwebteam/craft-spotler-transactional/blob/main/README.md",
		"source": "https://github.com/perfectwebteam/craft-spotler-transactional",
		"issues": "https://github.com/perfectwebteam/craft-spotler-transactional/issues"
	},
	"license": "MIT",
	"authors": [
		{ 
			"name": "Perfect Web Team",
			"homepage": "https://perfectwebteam.com"
		}
	],
	"require": {
		"php": "^8.0.2",
		"craftcms/cms": "^4.0|^5.0",
		"symfony/http-client": "^6.4",
		"nyholm/psr7": "^1.8",
		"flowmailer/flowmailer-php-sdk": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"perfectwebteam\\spotlertransactional\\": "src/"
		}
	},
	"extra": {
		"name": "Spotler Transactional",
		"handle": "spotler-transactional",
		"developer": "Perfect Web Team",
		"developerUrl": "https://perfectwebteam.com",
		"documentationUrl": "https://github.com/perfectwebteam/craft-spotler-transactional/blob/main/README.md",
		"changelogUrl": "https://raw.githubusercontent.com/perfectwebteam/craft-spotler-transactional/main/CHANGELOG.md",
		"class": "perfectwebteam\\spotlertransactional\\SpotlerTransactional"
	},
	"config": {
		"platform": {
			"php": "8.2"
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true,
			"php-http/discovery": true
		}
	}
}
